{
  "name": "khs1994/khsci",
  "description": "CI Based Docker Powered by PHP",
  "keywords": [
    "ci",
    "docker"
  ],
  "homepage": "https://github.com/khs1994-php/khsci",
  "type": "project",
  "license": "MIT",
  "authors": [
    {
      "name": "khs1994",
      "email": "khs1994@khs1994.com",
      "homepage": "https://khs1994.com"
    }
  ],
  "support": {
    "issues": "https://github.com/khs1994-php/khsci/issues"
  },
  "require": {
    "php": ">=7.1",
    "ext-PDO": "*",
    "ext-curl": "*",
    "ext-json": "*",
    "ext-yaml": "*",
    "doctrine/cache": "^1.6",
    "firebase/php-jwt": "^5.0",
    "khs1994/curl": "dev-master",
    "khs1994/docker": "dev-master",
    "khs1994/tencent-ai": "^18.03",
    "monolog/monolog": "^1.23",
    "nunomaduro/collision": "^2.0",
    "pimple/pimple": "~3.0",
    "symfony/console": "^3.4",
    "vlucas/phpdotenv": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.0",
    "phpunit/dbunit": "^4.0"
  },
  "config": {
    "platform": {
      "php": "7.1.0"
    },
    "optimize-autoloader": true,
    "sort-packages": true
  },
  "suggest": {
    "ext-xdebug": "*"
  },
  "autoload": {
    "psr-4": {
      "KhsCI\\": "src/KhsCI/",
      "App\\": "public/app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "KhsCI\\Tests\\": "tests/KhsCI/",
      "KhsCI\\Tests\\Plugins": "tests/Plugins/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "18.06-dev"
    }
  }
}
